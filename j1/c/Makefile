CC 	= gcc
CFLAGS  = -g -Wall -Werror -std=gnu11
OBJECTS = test_list.o olist.o types.o delta.o obj.o stack.o interp.o
BIN	= test

all: $(OBJECTS) parser
	$(CC) $(CFLAGS) $(OBJECTS) -o $(BIN)

parser: parse.o
	$(CC) $(CFLAGS) parse.o -o parser

%.o: %.c
	$(CC) $(CFLAGS) -c $^ -o $@


.PHONEY: clean
clean:
	rm -rf $(BIN) $(OBJECTS) parser
