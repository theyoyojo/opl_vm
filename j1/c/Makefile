CC 	= gcc
CFLAGS  = -g -Wall -std=gnu11
OBJECTS = test_list.o olist.o types.o delta.o
BIN	= test

all: $(OBJECTS)
	$(CC) $(CFLAGS) $(OBJECTS) -o $(BIN)

%.o: %.c
	$(CC) $(CFLAGS) -c $^ -o $@

.PHONEY: clean
clean:
	rm -rf $(BIN) $(OBJECTS)
